.article
  {jk_border-style}
  .article-meta
    color: #999999

.article-content
  a
    {jk_link-style}
  img
    {jk_img-style}
  blockquote 
    border-left: 2px solid #1E90FF
    margin: 0
    p, footer
      margin-left: 3%
  .hljs
    border: 2px solid #eeeeee
    border-radius: 9px
    padding: 3% 4%
  pre
    code
      font-family: "楷体"